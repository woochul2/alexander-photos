{
  "env": { "browser": true },
  "extends": ["airbnb-base"],
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "object-curly-newline": 0,
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 1
  }
}
